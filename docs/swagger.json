        "dto.CreateDeploymentRequestDTO": {
            "type": "object",
            "required": [
                "modelPackageID",
                "name",
                "predictionEnvID"
            ],
            "properties": {
                "modelPackageID": {
                    "description": "모델패키지 ID",
