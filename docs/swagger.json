                    "description": "예측 환경",
                    "type": "string",
                    "x-order": "4",
                    "example": "k8s-inference-staging"
                },
                "importance": {
                    "description": "배포중요도",
                    "type": "string",
                    "enum": [
                        "Low",
                        " Moderate",
                        " High",
                        " Critical"
                    ],
                    "x-order": "5",
